    "@aws-sdk/s3-request-presigner": "^3.335.0",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
